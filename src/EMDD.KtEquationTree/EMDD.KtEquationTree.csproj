<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <AssemblyName>EMDD.KtEquationTree</AssemblyName>
    <RootNamespace>EMDD.KtEquationTree</RootNamespace>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Authors>Marlon Dumal-is</Authors>
    <Company>EMDD</Company>
    <Description>Mathematical Expression Parser</Description>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/marlond18/KtEquationTree.git</PackageProjectUrl>
    <RepositoryUrl>https://github.com/marlond18/KtEquationTree.git</RepositoryUrl>
    <PackageIcon>emd2.png</PackageIcon>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Mathematical Expression Parser, Parser, C#, EMDD. Equation Parser, Equation</PackageTags>
    <PackageReleaseNotes>Initial Release</PackageReleaseNotes>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <Version>1.0.5.1</Version>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <WarningLevel>0</WarningLevel>
    <OutputPath>D:\Programming Work\EMDD.KtEquationTree\build\</OutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>D:\Programming Work\EMDD.KtEquationTree\build\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="EMDD.Kt.Extensions" Version="1.0.0.1" />
    <PackageReference Include="Pidgin" Version="3.2.1" />
  </ItemGroup>

  <ItemGroup>
    <None Update="emd2.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
</Project>
